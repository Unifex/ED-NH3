// galactic time
function updateGMTClockTime() {
   var f = new Date();
   $(".gmtclock").html( ("0" + f.getUTCHours()).slice(-2) + ":" +  ("0" + f.getUTCMinutes()).slice(-2) );
   var hoff = f.getUTCHours() - f.getHours();
   //var moff = f.getUTCMinutes() - f.getMinutes();
   var rectime = 0;
   var tappend = "";
   var galctitle = $('.gmtclock').data('tooltiptitle');
   var dayoff = $('.gmtclock').data('tooldayoff');
   var nowtime = $('.gmtclock').data('toolnowtime');
   for (i=0;i<25;i++) {
      rectime = hoff + i;
      tappend = "";
      if (rectime < 0) { rectime = 24 + rectime; tappend = "(-1 "+dayoff+")"; }
      if (rectime > 24) { rectime = rectime - 24; tappend = "(+1 "+dayoff+")"; }
      if (rectime == f.getUTCHours()) tappend = tappend + " ("+nowtime+")";
      galctitle = galctitle + ("0" + i).slice(-2) + ":00 = " + rectime + ":00 " + tappend + "<br>";
   }

   //$('#galclock').prop('title', galctitle);
   $('.gmtclock').data('tooltiptext', galctitle);
}

function startGMTClock() {
   setInterval(function () { updateGMTClockTime() }, 1000*30); //30sec
}


