// ajax and forms
function ajaxPopulateFromResult(data) {
   if (data) {
      if (data["dialogid"]) { // modal window/dialog
         var dialogid = data["dialogid"];
         var dialogtitle = data["dialogtitle"];
         var dialogcontent = data["dialogcontent"];
         $("#customdialog"+dialogid).dialog('option','title',dialogtitle);
         $("#customdialog"+dialogid).html(dialogcontent);
         $("#customdialog"+dialogid).dialog('open');
      }

      // in-page elements
      if (data["content"]) {
         $.each(data["content"], function(key, value) {
            $(key).html(value).hide().fadeIn(150);
         });
      }

      // in-page data
      if (data["data"]) {
         $.each(data["data"], function(key, tdata) {
            $.each(tdata, function(name, value) {
               $(key).data(name,value);
               $(key).attr("data-"+name,value);
            });
         });
      }

      if (!data["dialogid"]) {
         $('#customdialog1').dialog('close');
         scriptsInit();
         tablesInit();
      }

   };
}

function ajaxForm() {
   $("#ajaxform1,#ajaxform2,#ajaxform3,#ajaxform4,#ajaxform5").submit(function(e) {

       var form = $(this);
       var formdata = form.serialize();
       var selfpointer = $(this);
       //console.log(formdata);

       $.ajax({
              type: "POST",
              url: rootUrlAjax + "ajaxaction.php",
              data: formdata,
              dataType: 'json',
              success: function(data)
              {
                  //console.log(data);
                  selfpointer.unbind('submit');
                  ajaxPopulateFromResult(data);
              }
            });

       e.preventDefault();
   });
}


