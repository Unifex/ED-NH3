function ajaxToggleButton() {

   var refact = $(this).data('action');
   var reftype = $(this).data('type');
   var refid = $(this).data('id');
   var refid2 = $(this).data('id2');
   var refid3 = $(this).data('id3');
   var refid4 = $(this).data('id4');
   var isoneshot = $(this).data('oneshot');
   var reftexttrue = $(this).data('texttrue');
   var reftextfalse = $(this).data('textfalse');
   var selfpointer = $(this);

   //console.log(reftype);
   if (reftype == 1) {
      if (isoneshot == 1) {
         $(this).addClass('toggleactionbuttonset');
      }
      $(this).addClass('toggleactionbuttontrue');
      $(this).removeClass('toggleactionbuttonfalse');
      $(this).data('type',0);
   } else {
      $(this).addClass('toggleactionbuttonfalse');
      $(this).removeClass('toggleactionbuttontrue');
      $(this).data('type',1);
   }

   $(this).addClass('ajaxcursorprogress');

   $.ajax({
      type:"GET",
      url: rootUrlAjax + "ajaxaction.php",
      data:'type='+refact+'&reftype='+reftype+'&refid='+refid+'&refid2='+refid2+'&refid3='+refid3+'&refid4='+refid4,
      dataType: 'json',
      success: function(data) {
          selfpointer.data('acttype', (1 - selfpointer.data('acttype')) );
          if (reftype == 1) {
             selfpointer.text(reftexttrue);
          } else {
             selfpointer.text(reftextfalse);
          }

          if (selfpointer.data('oneshot') == 1) {
             selfpointer.unbind("clickable");   // disable one-shot buttons
          }

          selfpointer.removeClass('ajaxcursorprogress');

          //console.log(data);
          ajaxPopulateFromResult(data);

          // re-init buttons in lists retrieved
          $('.likebutton:not(.clickbound)').unbind().click(votingButton);
          $('.likebutton:not(.clickbound)').addClass('clickbound');
          $('.filteractionbutton:not(.clickbound)').unbind().click(ajaxToggleButton);
          $('.toggleactionbutton:not(.clickbound)').unbind().click(ajaxToggleButton);
          $('.deleteajaxbutton:not(.clickbound)').unbind().click(ajaxDeleteButton);
          $('.toggleactionbutton:not(.clickbound),.deleteajaxbutton:not(.clickbound)').addClass('clickbound');
      }
  });
}

